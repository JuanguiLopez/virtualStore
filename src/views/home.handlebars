<main style="padding-left: 20px;">
  <h1>Hola {{user.name}}!</h1>

  <div class="navbar">
    <p>Email: {{user.email}}</p>
    {{#if user.age}}
      <p>Edad: {{user.age}}</p>
    {{/if}}
    <p>Rol: {{user.role}}</p>
  </div>
  <br />
  <h3>Estos son los productos disponibles</h3>

  <table>
    <thead>
      <tr>
        <th>id</th>
        <th>Nombre</th>
        <th>Descripci√≥n</th>
        <th>Precio</th>
        <th>Stock</th>
        <th>Categoria</th>
      </tr>
    </thead>
    <tbody id="realTimeTable">
      {{#each productos}}
        <script>console.log(productos)</script>
        <tr>
          <td>{{this._id}}</td>
          <td>{{this.title}}</td>
          <td>{{this.description}}</td>
          <td>${{this.price}}</td>
          <td>{{this.stock}}</td>
          <td>{{this.category}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</main>

